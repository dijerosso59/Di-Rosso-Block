{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "di-rosso-block/di-rosso-editable",
  "version": "1.0",
  "title": "Di Rosso Editable",
  "icon": "edit",
  "category": "text",
  "keywords": [
    "C'est Di Rosso",
    "Editable",
    "Gutenberg"
  ],
  "description": "Tu peux editer un bloc, remercie moi !",
  "supports": {
    "html": false
  },
  "attributes": {
    "title": {
      "type": "string",
      "source": "text",
      "selector": ".title"
    },
    "content": {
      "type": "string",
      "source": "text",
      "selector": ".text"
    },
    "image": {
      "type": "string",
      "source": "attribute",
      "selector": "img",
      "attribute": "src"
    }
  },
  "textdomain": "di-rosso-block",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}